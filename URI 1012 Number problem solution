#include <stdio.h>
#define pi 3.14159
main()
{
    double AREA,A,TRA,B,C,REC,T,Q;
    scanf("%lf %lf %lf",&A,&B,&C);
    T=0.5*A*C;
    AREA=pi*C*C;
    TRA=0.5*(A+B)*C;
    Q=B*B;
    REC=A*B;
    printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n",T,AREA,TRA,Q,REC);
    return 0;
    }
