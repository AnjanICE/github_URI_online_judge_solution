#include <stdio.h>
#include <math.h>
int main(){
    int n,i,x,y,z,a,b,c;
    scanf("%d",&n);
    for(i=0;i<n;i++){
            x=i+1;
            y=pow((i+1),2);
            z=pow((i+1),3);
            a=i+1;
            b=y+1;
            c=z+1;
            printf("%d %d %d\n",x,y,z);
            printf("%d %d %d\n",a,b,c);
    }
    return 0;
}
