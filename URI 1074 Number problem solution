#include <stdio.h>
main()
{
    int T,N;
    scanf("%d",&T);
        while(T-- && scanf("%d",&N)!=EOF){
        if(N%2==0 && N>0)
            printf("EVEN POSITIVE\n");
        else if(N%2!=0 && N>0)
            printf("ODD POSITIVE\n");
        else if(N%2!=0 && N<0)
            printf("ODD NEGATIVE\n");
        else if(N%2==0 && N<0)
            printf("EVEN NEGATIVE\n");
        else if(N==0)
            printf("NULL\n");
            }
        return 0;
}
