#include <stdio.h>
main()
{
    int A,B,C,D,E,F,G,H,Num,I,J,K,L,n,p;
    scanf("%f",&Num);
    printf("NOTAS:\n",Num);
    A=Num/100;
    printf("%d nota(s) de R$ 100.00\n",A);
    B=(Num-(A*100))/50;
    printf("%d nota(s) de R$ 50.00\n",B);
    C=(Num-(A*100)-(B*50))/20;
    printf("%d nota(s) de R$ 20.00\n",C);
    D=(Num-(A*100)-(B*50)-(C*20))/10;
    printf("%d nota(s) de R$ 10.00\n",D);
    E=(Num-(A*100)-(B*50)-(C*20)-(D*10))/5;
    printf("%d nota(s) de R$ 5.00\n",E);
    F=(Num-(A*100)-(B*50)-(C*20)-(D*10)-(E*5))/2;
    printf("%d nota(s) de R$ 2.00\n",F);

    printf("MOEDAS:\n");
    G=(Num-(A*100)-(B*50)-(C*20)-(D*10)-(E*5)-(F*2))/1;
    printf("%d moeda(s) de R$ 1.00\n",G);
        H=Num/50;
    printf("%d moeda(s) de R$ 0.50\n",H);
     I=((Num-(H*50))/25);
    printf("%d moeda(s) de R$ 0.25\n",I);
     J=((Num-(H*50)-(I*25))/10);
    printf("%d moeda(s) de R$ 0.10\n",J);
     K=(((Num-(H*50)-(I*25)-(J*10)))/5);
    printf("%d moeda(s) de R$ 0.05\n",K);
     L=(((Num-(H*50)-(I*25)-(J*10)-(K*5)))/1);
    printf("%d moeda(s) de R$ 0.01\n",L);
    return 0;
    }
