#include <stdio.h>
main()
{
    int n,i,sum;
    while(scanf("%d",&n)==1)
   {
      sum=0;
       if(n==0)
            break;
      if(n%2==1 || n%2==-1)
      {
         n++;
      }
      for(i=n;i<n+9;i++)
      {
         if(i%2==0)
            sum=sum+i;
      }

      printf("%d\n",sum);
   }
   return 0;
}
