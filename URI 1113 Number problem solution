#include <stdio.h>
int main(){
    int M,N,Sum,i,aux;
    while(scanf("%d %d",&M,&N),(M>0)&&(N>0)){
        Sum=0;
       if(M>N){
            aux=M;
            M=N;
            N=aux;
       }
        for(i=M;i<=N;i++){
            printf("%d ",i);
             Sum+=i;
        }
        printf("Sum=%d\n",Sum);
    }
    return 0;
}
