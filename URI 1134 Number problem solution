#include <stdio.h>
int main(){
    int n,A=0,G=0,D=0;
    while(scanf("%d",&n)){
                if(n==1) A++;
                else if(n==2) G++;
                else if(n==3) D++;
                else if(n==4) break;
                else continue;
    }
     printf("MUITO OBRIGADO\n");
    printf("Alcool: %d\n",A);
    printf("Gasolina: %d\n",G);
    printf("Diesel: %d\n",D);
    return 0;
}
